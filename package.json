{
	"name": "bpsr-logs",
	"version": "0.1.0",
	"description": "",
	"type": "module",
	"packageManager": "bun@1.3.6",
	"scripts": {
		"build": "bun run --bun vite build",
		"check": "bun run --bun svelte-kit sync && bun run --bun svelte-check --tsconfig ./tsconfig.json && cargo check --manifest-path src-tauri/Cargo.toml",
		"check:watch": "bun run --bun svelte-kit sync && bun run --bun svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "cargo clean --manifest-path src-tauri/Cargo.toml",
		"dev": "bun run --bun vite dev",
		"format": "prettier --write . && cargo fmt --manifest-path src-tauri/Cargo.toml",
		"format:check": "prettier --check . && cargo fmt --check --manifest-path src-tauri/Cargo.toml",
		"lint": "eslint .",
		"preview": "vite preview",
		"tauri": "tauri",
		"update": "cargo update --manifest-path src-tauri/Cargo.toml",
		"upgrade": "ncu -u && bun install && cargo install cargo-edit && cargo upgrade --incompatible allow --manifest-path src-tauri/Cargo.toml && cargo update --manifest-path src-tauri/Cargo.toml"
	},
	"license": "MIT",
	"dependencies": {
		"@tanstack/table-core": "^8.21.3",
		"@tauri-apps/api": "^2.9.1",
		"@tauri-apps/plugin-autostart": "^2.5.1",
		"@tauri-apps/plugin-clipboard-manager": "^2.3.2",
		"@tauri-apps/plugin-global-shortcut": "^2.3.1",
		"@tauri-apps/plugin-opener": "^2.5.3",
		"@tauri-apps/plugin-os": "^2.3.2",
		"@tauri-apps/plugin-store": "^2.4.2",
		"@tauri-apps/plugin-updater": "^2.9.0",
		"@tauri-store/svelte": "^3.1.0",
		"html2canvas-pro": "^1.6.4",
		"tippy.js": "^6.3.7"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.1",
		"@eslint/js": "^9.39.2",
		"@iconify-json/logos": "^1.2.10",
		"@iconify-json/lucide": "^1.2.86",
		"@iconify/json": "^2.2.429",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.562.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@tauri-apps/cli": "^2.9.6",
		"@types/node": "^25.0.9",
		"bits-ui": "^2.15.4",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"formsnap": "^2.0.1",
		"globals": "^17.0.0",
		"prettier": "^3.8.0",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.46.4",
		"svelte-check": "^4.3.5",
